{
  "name": "@ianwalter/sled",
  "version": "2.1.1",
  "description": "A JavaScript micro-library for animating an element's height to create a slide effect",
  "main": "dist/sled.js",
  "module": "dist/sled.m.js",
  "iife": "dist/sled.iife.js",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint index.js test.js",
    "build:site": "NODE_ENV=production webpack --env.singleRun",
    "dist": "dist --babel --no-iife",
    "iife": "dist --name Sled --babel --inline --no-cjs --no-esm",
    "pretest": "npm run dist && npm run iife",
    "test": "ava"
  },
  "keywords": [
    "slide",
    "height",
    "transition",
    "toggle",
    "collapse"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/sled.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/sled/issues"
  },
  "homepage": "https://sled.iankwalter.com",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.3.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@fullhuman/purgecss-loader": "^1.0.0",
    "@ianwalter/dist": "^1.5.1",
    "@ianwalter/dracula": "^2.0.0",
    "@ianwalter/prism-loader": "^1.1.0",
    "@ianwalter/puppeteer-helper": "^1.0.0",
    "ava": "^1.2.0",
    "bootstrap": "^4.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "np": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "browserslist": [
    "> 0.5%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "standard"
    ],
    "globals": {
      "Sled": true
    }
  }
}
